<script type="text/javascript">
$(function(){
  
  // show/hide report filters and layers boxes on home page map
  $("a.toggle").toggle(
    function() { 
      $($(this).attr("href")).show();
      $(this).addClass("active-toggle");
    },
    function() { 
      $($(this).attr("href")).hide();
      $(this).removeClass("active-toggle");
    }
  );
  
});

</script>
